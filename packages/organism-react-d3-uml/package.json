{
  "name": "organism-react-d3-uml",
  "version": "0.1.0",
  "description": "React D3 UML",
  "repository": "react-atomic/react-atomic-organism",
  "main": "./build/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "d3-lib": "*",
    "getoffset": "*",
    "organism-react-graph": "*",
    "react-atomic-molecule": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "react": "^16.x",
    "react-dom": "^16.x"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src && BABEL_ENV=build babel ui -d build/cjs/ui",
    "build:es": "BABEL_ENV=es babel src -d build/es/src && BABEL_ENV=es babel ui -d build/es/ui",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
