{
    "env": {
        "build": {
            "presets": [
              "env",
              "react",
              "stage-0"
            ],
            "plugins": [
              "add-module-exports",
              "system-import-transformer",
              "transform-react-constant-elements",
              ["transform-runtime", {
                "moduleName": "reshow-runtime",
                "polyfill": false,
                "regenerator": false
                }
              ],
              ["transform-react-remove-prop-types", {"mode": "wrap"}]
            ]
        },
        "es": {
            "presets": [
              ["env", {"modules": false}],
              "react",
              "stage-0"
            ],
            "plugins": [
              "transform-react-constant-elements",
              ["transform-runtime", {
                "moduleName": "reshow-runtime/es",
                "polyfill": false,
                "regenerator": false
                }
              ],
              ["transform-react-remove-prop-types", {"mode": "wrap"}]
            ]
        }
    }
}
